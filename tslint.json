{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark": [
            true,
            "single"
        ],
        "eofline": false,
        "arrow-parens": false,
        "trailing-comma": false,
        "only-arrow-functions": false,
        "space-before-function-paren": false,
        "ordered-imports": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}